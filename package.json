    "e2e": "cross-env BABEL_ENV=development NODE_ENV=production bash build/shell.test.e2e.sh",
    "dev-e2e": "casperjs test --concise ./test/e2e/specs",
    "dev-cover": "chrome test/unit/coverage/lcov-report/index.html"
  },
